public class implicitCasting{
    public static void main(String args[]){
        //Implicit Casting
        //byte >> short >> int >> long
        
        // double x = 1.1;
        // int y = (int)(x) + 2;
        // System.out.println(y);
        
        // String x = "1";
        // int y = Integer.parseInt(x) + 2;
        // System.out.println(y);
        
        String x = "1.1";
        double y = Double.parseDouble(x) + 2;
        System.out.println(y);
    }
}
